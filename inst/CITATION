citHeader("To cite grpsel in publications use:")

citEntry(
  entry       = "Article",
  title       = "Group selection and shrinkage with application to sparse semiparametric modeling",
  author      = personList(as.person("Ryan Thompson"), as.person("Farshid Vahid")),
  journal     = "arXiv:2105.12081",
  year        = "2021",
  textVersion =
  paste("Ryan Thompson, Farshid Vahid (2021).",
        "Group selection and shrinkage with application to sparse semiparametric modeling",
        "arXiv:2105.12081")
)
